    "url_metadata": "https://www.adaderana.lk/news/112882/tariffs-and-conflict-causing-major-volatility-in-shipping-industry-says-un-trade-agency",
    "lang": "en",
    "newspaper_id": "adaderanalk",
    "time_ut": 1758726660.0,
    "chunk_id": "2025-09-24-adaderanalk-e1a84c24-0000",
