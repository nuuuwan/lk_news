    "url_metadata": "http://island.lk/sls-construction-sector-bleeding-billions-due-to-weak-cost-control-mechanisms/",
    "lang": "en",
    "newspaper_id": "islandlk",
    "time_ut": 1765215660.0,
    "chunk_id": "2025-12-08-islandlk-02b3aef5-0000",
